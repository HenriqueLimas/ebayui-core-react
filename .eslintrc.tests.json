{
    "extends": [
        ".eslintrc"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.tests.json"
    },
    "ignorePatterns": [
        "!**/__tests__/**"
    ],
    "rules": {
        "max-len": "off",
        "no-nested-ternary": "off",
        "no-extra-parens": "off",
        "react/no-unescaped-entities": "off",
        "@typescript-eslint/explicit-module-boundary-types": "off"
    }
}
